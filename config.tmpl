XCOMM	$Id$

XCOMM
XCOMM disable sound
XCOMM
#undef	NO_SOUND

XCOMM
XCOMM disable mercury unit
XCOMM
#undef	NO_MERCURY

XCOMM
XCOMM for Opt.
XCOMM
XCOMM CDEBUGFLAGS= -O3 -march=i686 -mcpu=pentiumpro
XCOMM CDEBUGFLAGS+= -funroll-loops -ffast-math
XCOMM CDEBUGFLAGS+= -fomit-frame-pointer
XCOMM for gcc 3
XCOMM CDEBUGFLAGS+= -msse -mfpmath=sse

XCOMM
XCOMM for DEBUG
XCOMM
XCOMM CDEBUGFLAGS+= -g
XCOMM CDEBUGFLAGS+= -W -Wall -Wuninitialized
XCOMM CDEBUGFLAGS+= -Wunused
XCOMM CDEBUGFLAGS+= -DINLINE=
