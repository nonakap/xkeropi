XCOMM	$Id$

XCOMM disable sound
#undef	NO_SOUND

XCOMM disable mercury unit
#define	NO_MERCURY

XCOMM for Opt.
CDEBUGFLAGS= -O3 -m486
CDEBUGFLAGS+= -funroll-loops -fomit-frame-pointer

XCOMM for DEBUG
XCOMM CDEBUGFLAGS= -g -O2
XCOMM CDEBUGFLAGS+= -g
XCOMM CDEBUGFLAGS+= -W -Wall
XCOMM CDEBUGFLAGS+= -DINLINE=
